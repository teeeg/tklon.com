---
pageable: true
per_page: 5
layout: layouts/default
---

<% page_articles.each do |article| %>
  <%= partial("partials/summary", :locals => {:current_page => article}) %>
<% end %>

<footer>
  <p>
    <% if !prev_page.nil? %>
      <%=link_to '< Prev', prev_page %>
      <%= link_to 'tklon.com', '/' %>
    <% end %>
    <% if !next_page.nil? %>
      <%= link_to 'Next >', next_page %>
    <% end %>
  </p>
</footer>
